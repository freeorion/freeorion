@total_ordering
class _NumberValue:
    def __add__(self, other) -> _NumberValue: ...
    def __radd__(self, other) -> _NumberValue: ...
    def __sub__(self, other) -> _NumberValue: ...
    def __rsub__(self, other) -> _NumberValue: ...
    def __mul__(self, other) -> _NumberValue: ...
    def __rmul__(self, other) -> _NumberValue: ...
    def __floordiv__(self, other) -> _NumberValue: ...
    def __rfloordiv__(self, other) -> _NumberValue: ...
    def __truediv__(self, other) -> _NumberValue: ...
    def __rtruediv__(self, other) -> _NumberValue: ...
    def __pow__(self, other) -> _NumberValue: ...
    def __rpow__(self, other) -> _NumberValue: ...
    def __gt__(self, other) -> bool: ...
    def __eq__(self, other) -> bool: ...

class _FloatValue(_NumberValue): ...
